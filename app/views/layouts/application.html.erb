        <link rel="shortcut icon" href="/favicon.ico">
    </head>
    <body>
        <div class="container-fluid" >
            <div class="row" style="min-height:620px;">
                <%= render "common/navbar"%>
                <div class="service-space">
                </div>
                <%= render "common/flashes"%>
                <%= yield %>
            </div>
        </div>
        <%= render "common/footer"%>
        <script>
            window.intercomSettings = {
                app_id: "a1lf3agy"
              };
        </script>
        <script>
            (function(){var w=window;var ic=w.Intercom;if(typeof ic==="function"){ic('reattach_activator');ic('update',intercomSettings);}else{var d=document;var i=function(){i.c(arguments)};i.q=[];i.c=function(args){i.q.push(args)};w.Intercom=i;function l(){var s=d.createElement('script');s.type='text/javascript';s.async=true;s.src='https://widget.intercom.io/widget/a1lf3agy';var x=d.getElementsByTagName('script')[0];x.parentNode.insertBefore(s,x);}if(w.attachEvent){w.attachEvent('onload',l);}else{w.addEventListener('load',l,false);}}})()
        </script>
    </body>
</html>
